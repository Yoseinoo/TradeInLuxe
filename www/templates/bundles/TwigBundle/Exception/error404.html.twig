{% extends 'base.html.twig' %}
{% block title %}Erreur 404 - Page non trouvée
{% endblock %}
{% block body %}
	<section class="erreur">
		<div class="erreurContainer">
			<h1 class="erreurContainerTitle">ERREUR 404</h1>
			<img src="{{ asset('build/images/logo/settings.svg') }}" alt="image erreur" class="erreurContainerImage">
			<p class="erreurContainerTexte">La page que vous recherchez n'a pas été trouvée.</p>

			<a href="{{ path('app_accueil') }}" class="erreurContainerButton">Page d'accueil</a>
		</div>
	</section>
{% endblock %}
